      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="GefProjectDB">

  <head><title></title></head>
  <body>
  
    <metal:fill fill-slot="main">
      <metal:main_macro define-macro="main">
     <tal:redirect tal:condition="python: context.portal_type =='Financials'" >
    <b tal:define="dummy python:container.REQUEST.RESPONSE.redirect(here.aq_parent.absolute_url() + '/fmi_view')">
    redirect to parent!
    </b>
    
  </tal:redirect>
       <tal:fmilisting tal:define="fmiresults python:container.portal_catalog(portal_type = ['Financials'],review_state=['published','visible'],path='/'.join(here.getPhysicalPath()));">

	   <!--h4 tal:content="title_string | here/title_or_id" /-->
	   <h1> Project Category: 
	        <tal:entry tal:repeat="category fmiresults">
	        	<a tal:define="co category/getObject" tal:attributes="href string:#${co/FinanceCategory}" tal:content="co/FinanceCategory"> 
	        	PDFA, PDFB, MSP, ... </a>
	        </tal:entry>
	   
       </h1>
       <a href="#" tal:attributes="href python:here.absolute_url()+'/createObject?type_name=Financials';">
        <img SRC="add_fmi.jpg"></a>
	   <div metal:use-macro="here/document_actions/macros/document_actions" >
	   	   	              Document actions (print, sendto etc)
       </div>
       <tal:fmis  tal:repeat="category fmiresults">
        <tal:entry  tal:define="co category/getObject"> 
        <a tal:attributes="name co/FinanceCategory">
       	<h2 tal:content="string:Category ${co/FinanceCategory}"> Category: FSP </h2>
        </a>
        <form tal:attributes="action string:${co/absolute_url}/base_edit" method="link">
         <input type="submit" value="Edit">
        </form> 
        <table>
       	
        <tr><td>
       	<table class="financialinfotable" id="financialcore" width='100%'>
       	  <tbody>
       	  	<tr  tal:define="field python:co.Schema()['title']; accessor python:getattr(co, field.accessor);
       	  											mode string:view;
		   	   	   		   							widget_view python:co.widget(field.getName(), mode=mode, use_label=1);"
		   	   	   		   							tal:condition="python:not accessor()==''">
       	  	<td bgcolor="#d6d7d9">Project Title:</td> <td bgcolor="#f7f9fa" >  <metal:block use-macro="widget_view" /> </td>
       	  	</tr>
            <tr  tal:define="field python:co.Schema()['FinanceCategory']; accessor python:getattr(co, field.accessor);
       	  											mode string:view;
		   	   	   		   							widget_view python:co.widget(field.getName(), mode=mode, use_label=1);"
		   	   	   		   							tal:condition="python:not accessor()==''">
       	  	<td bgcolor="#d6d7d9">Project Category:</td> <td bgcolor="#f7f9fa">  <metal:block use-macro="widget_view" /> </td>
       	  	</tr>
       	  	<tr tal:define="field python:co.Schema()['PMSNumber']; accessor python:getattr(co, field.accessor);
       	  											mode string:view;
		   	   	   		   							widget_view python:co.widget(field.getName(), mode=mode, use_label=1);"
		   	   	   		   							tal:condition="python:not accessor()==''">
       	  	<td bgcolor="#d6d7d9">PMS Number:</td> <td bgcolor="#f7f9fa" >  <metal:block use-macro="widget_view" /></td>
       	  	</tr>
       	  	<tr>
       	  	<td bgcolor="#d6d7d9">IMIS Number:</td> <td bgcolor="#f7f9fa" tal:content="co/IMISNumber"> 477862 </td>
       	  	</tr>
       	  	<tr  tal:define="field python:co.Schema()['GEFProjectAllocation']; accessor python:getattr(co, field.accessor);
       	  											mode string:view;
		   	   	   		   							widget_view python:co.widget(field.getName(), mode=mode, use_label=1);"
		   	   	   		   							tal:condition="python:not accessor()==''">
       	  	<td bgcolor="#d6d7d9">Total GEF Allocation: </td> <td bgcolor="#f7f9fa"> <metal:block use-macro="widget_view" /> </td>
       	  	</tr>
