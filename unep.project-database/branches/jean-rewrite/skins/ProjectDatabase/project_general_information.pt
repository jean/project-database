         </table>             
         </center><br>
            
        <!--End of Mode of Implementation-->

         <!--Implementation Status-->
      <!-- Project Implementation Status-->
        <table tal:define="piresults python:container.portal_catalog(portal_type = ['ProjectImplementation'],review_state=['published','visible'],path='/'.join(here.getPhysicalPath()));" 
            class="financialinfotable" 
            id="ImplementationStatus" 
            border="1" 
            cellpadding="0" 
            cellspacing="3" 
            style="border-collapse: collapse" bordercolor="#D6D7D9">
        	<thead> 
                <tr>
       	  		<th bgcolor="#78a5d1"  colspan="4" >Project Implementation Status</th>
       	  	    </tr>
            </thead>
            <tbody  tal:condition="piresults">
                <tr><td width=30% bgcolor="#DFEAF4">Year</td>
                    <td width=70% bgcolor="#DFEAF4">Remark</td>
                </tr>
       	  	    <tal:entry tal:repeat="pi piresults">
       	  	 	    <tr tal:define="pio pi/getObject">
       	  	 		    <td tal:on-error="nothing" 
                            tal:define="field python:pio.Schema()['title']; accessor python:getattr(pio, field.accessor);
       	  								mode string:view;
		   	   	   		   				widget_view python:pio.widget(field.getName(), mode=mode, use_label=1);"
		   	   	   		   	tal:condition="python:not accessor()==''" > 
                            <metal:block use-macro="widget_view" />
                        </td>
       	  	 		    <td tal:on-error="nothing"  
                            tal:define="field python:pio.Schema()['Remark']; accessor python:getattr(pio, field.accessor);
                                        mode string:view;
                                        widget_view python:pio.widget(field.getName(), mode=mode, use_label=1);"
                            tal:condition="python:not accessor()==''"> 
                                <metal:block use-macro="widget_view" />
                        </td>
    	  	 	    </tr>
       	  	    </tal:entry>
            </tbody>
            </table>
         <!--End of implementation status-->  
         
         <!--Sub projects-->         
              
        <table tal:define="subprojresults python:container.portal_catalog(portal_type = ['SubProject'], review_state=['published','visible'], path='/'.join(here.getPhysicalPath()));" 
            class="financialinfotable" 
            id="SubProject" 
            border="1" 
            cellpadding="0" 
            cellspacing="3" 
            style="border-collapse: collapse" 
            bordercolor="#D6D7D9" 
            width="100%">
        	<thead> 
                <tr>
       	  		    <th bgcolor="#78a5d1"  colspan="4" >Sub Projects</th>
       	  	    </tr>
            </thead>	
            <tbody  tal:condition="subprojresults">   
                <tr><td width="50%" bgcolor="#DFEAF4">Title</td>
                    <td width="25%" bgcolor="#DFEAF4">IMIS Number</td>
                    <td width="25%" bgcolor="#DFEAF4">PMS Number</td>
                </tr>
       	  	 <tal:entry tal:repeat="sp subprojresults">
       	  	 	<tr tal:define="subproj sp/getObject;
                                        sp_url sp/getURL;
                                        subproj_url string:$sp_url/subproj_view;">


                    <td>
                        <a href="#" tal:attributes="href subproj_url"
                                tal:content="python:sp.Title or sp.getId" />
                    </td>
                    <td tal:on-error="nothing" 
                        tal:define="field python:subproj.Schema()['IMISNumber']; 
                                    accessor python:getattr(subproj , field.accessor)" 
                        tal:condition="python:not accessor()==''">
                            <span tal:replace="structure accessor"/>
                    </td>
                    <td tal:on-error="nothing" 
                        tal:define="field python:subproj .Schema()['PMSNumber']; 
                                    accessor python:getattr(subproj , field.accessor)" 
                        tal:condition="python:not accessor()==''">
                        <span tal:replace="structure accessor"/>
                    </td>
                </tr>
       	  	 </tal:entry>
            </tbody>
            </table>
		   
	   <!--End of sub projects-->
          
        
	

      

      </metal:main_macro>
    </metal:fill>
  </body>
</html>
